<body style="overflow-x: hidden;">
  <div class="pic1">
    <%= link_to canadas_path do %>
    <%= image_tag "2" %>
    <%end%>
  </div>

  <p  class="pic2"></p>
  <div class="dropdown-show">
    <a class="btn btn-light dropdown-toggle" href="#" role="button" id="                            dropdownMenuLink" data-toggle="dropdown">Menu
    </a>

    <div class="dropdown-menu" >
      <%= link_to 'visit', canada_visit_path(:visa), class: "dropdown-item" %>

      <%= link_to 'immigrant', canada_immigrant_path(:visa), class: "dropdown-item" %>

      <%= link_to 'study', canada_study_path(:visa), class: "dropdown-item" %>

      <%= link_to 'work', canada_work_path(:visa), class: "dropdown-item" %>
    </div>
  </div>
  <div class="container">
   <h1 class="oo">List of all customer Canada</h1>

    <%= link_to 'New Apply', new_canada_path %><br><br>
    <div class="table-responsive">
      <table class="table">
        <tr>
          <th>First Name</th>
          <th>Nationality</th>
          <th>Passport no</th>
          <th>Visa category</th>
          <th>Application status</th>
        </tr>
        <% @canadas.each do |canada| %>
        <tr>
          <td><%= canada.first_name %></td>
          <td><%= canada.nationality %></td>
          <td><%= canada.passport_no %></td>
          <td><%= canada.visa_category %></td>
          <td><%= canada.application_status %></td>
          <td><%= link_to 'show', canada_path(canada) %></td>
          <td><%= link_to 'edit', edit_canada_path(canada) %></td>
          <td><%= link_to "Delete", canada_path(canada), :confirm => "Are you sure?", :method => :delete %></td>
        </tr>
        <%end%>
      </table>
    </div>
  </div>
        <div class="brand1">
      <div class="container" style="margin-top: 26px; margin-bottom: 1px;">
        <div class="row">
          <nav class="col-sm-10 ftr-urlt-lnk">
            <ul class="list-group flex-md-row" style="margin-top: 18px; ">
              <li style="margin-right: 27px;">social-media</li>
              <li style="margin-right: 27px;">Mobile-Application</li>
              <li style="margin-right: 27px;">canada.ca</li>
              <li style="margin-right: 27px;">Term&condition</li>
              <li>Privacy</li>
            </ul>
          </nav>
          <div class="col-xs-6 col-md-2 text-right" style="padding-top: 10px;">
           <%= image_tag "last" %>
          </div>
        </div>
      </div>
    </div>
</body>
